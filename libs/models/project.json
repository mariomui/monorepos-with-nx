{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/models/src",
  "projectType": "library",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/models/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/models"],
      "options": {
        "jestConfig": "libs/models/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "models-ui-view": {
      "executor": "nx:run-commnads",
      "options": {
        "commands": [
          "npx prisma studio --schema ./libs/models/src/prisma/schema.prisma"
        ],
        "cwd": "libs/models"
      }
    },
    "migrate-dev": {
      "executor": "nx:run-commands",
      "options": {
          "commands": [
            "npx prisma migrate dev --experimental --schema ./src/prisma/schema.prisma"
          ],
          "cwd": "libs/models",
          "parallel": false
      }
    },
    "migrate-reset": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx prisma migrate reset --force --schema ./src/prisma/schema.prisma"],
        "cwd": "libs/models"
      }
    },
    "prisma-studio": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["npx prisma studio --schema ./src/prisma/schema.prisma"],
        "cwd": "libs/models"
      }
    },
    "migrate-sync": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "npx prisma db pull --schema ./src/prisma/schema.prisma",
        "npx prisma migrate dev --name introspected_change --schema ./src/prisma/schema.prisma"
      ],
        "cwd": "libs/models"
      }
    }
  },
  "tags": []
}
