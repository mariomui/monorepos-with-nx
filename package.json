{
  "name": "bookspear",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "start:dev": "run-s tab:begin:shell tab:begin:api tab:begin:prisma-studio",
    "tab:begin:shell": "ttab -t 'Building shell' npm run nx -- run console-root:serve",
    "tab:begin:api": "ttab -t 'Serving api' npm run nx -- run api:serve",
    "tab:begin:prisma-studio": "ttab -t 'Running studio' npm run nx -- run models:prisma-studio",
    "nx": "nx",
    "nxformat": "nx format:write --files=./debugs/**/*.json",
    "console-root:build:prod": "nx build console-root --prod -- --memoryLimit=8192",
    "console-root:build:prod:stats": "nx build console-root --prod --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "console-root:build:dev:stats": "nx build console-root --skip-nx-cache -- --memoryLimit=8192 --statsJson",
    "analyze:prod:stats": "run-s \"console-root:build:prod:stats\" \"webpack-bundle-analyzer\"",
    "analyze:dev:stats": "run-s \"console-root:build:dev:stats\" \"webpack-bundle-analyzer\"",
    "webpack-bundle-analyzer": "webpack-bundle-analyzer dist/apps/console-root/stats.json"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} ./src/prisma/seed.ts"
  },
  "private": true,
  "dependencies": {
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@primer/octicons": "^17.5.0",
    "@prisma/client": "^4.1.0",
    "core-js": "^3.6.5",
    "express": "4.18.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-router-dom": "6.3.0",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.19.1",
    "@emotion/babel-plugin": "11.10.0",
    "@nrwl/cli": "14.7.5",
    "@nrwl/cypress": "14.7.5",
    "@nrwl/eslint-plugin-nx": "14.7.5",
    "@nrwl/express": "14.7.5",
    "@nrwl/jest": "14.7.5",
    "@nrwl/linter": "14.7.5",
    "@nrwl/node": "14.7.5",
    "@nrwl/nx-cloud": "14.6.2",
    "@nrwl/react": "14.7.5",
    "@nrwl/web": "14.7.5",
    "@nrwl/workspace": "14.7.5",
    "@testing-library/react": "13.3.0",
    "@types/express": "4.17.13",
    "@types/jest": "28.1.8",
    "@types/node": "18.7.14",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.5",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.33.1",
    "@typescript-eslint/parser": "5.33.1",
    "babel-jest": "28.1.3",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.0",
    "eslint": "~8.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "eslint-plugin-react": "7.31.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "execa": "^6.1.0",
    "jest": "28.1.3",
    "jest-environment-jsdom": "28.1.1",
    "npm-run-all": "^4.1.5",
    "nx": "14.7.5",
    "prettier": "^2.6.2",
    "prisma": "^4.1.0",
    "react-test-renderer": "18.2.0",
    "run-all": "^1.0.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "ttab": "^0.7.3",
    "typescript": "~4.7.2",
    "webpack-bundle-analyzer": "^4.6.1"
  }
}